#include <iostream>
#include <stack>
#include <string>
#include <algorithm>
using namespace std;

void prefixToPostfix(string infix,string prefix){
    stack<char> s;
    string answer;
    char oper1,oper2;
    int i=0;
    while(infix[i]!='\0'){
        if((infix[i]>=65 && infix[i]<=90) || (infix[i]>=97 && 
infix[i]<=122)){
            answer[i]=infix[i];
        }
        if((infix[i] == '+' || infix[i] == '-' || infix[i] == '*' || 
infix[i] == '/')){
            oper1=answer.top();
            infix.pop();
            oper2=answer.top();
            infix.pop();
        }
    }
}

int main()
{
    cout<<"Hello World"<<endl;
    string infix,prefix;
    getline(cin,infix);
    reverse(infix.begin(),infix.end());
    cout<<infix;
    prefixToPostfix(infix,prefix);
    
    return 0;
}

